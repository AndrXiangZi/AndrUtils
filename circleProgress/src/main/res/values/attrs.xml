<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressBar">
        <!-- 最大进度-->
        <attr name="max" format="integer" />
        <!--当前进度-->
        <attr name="progress" format="integer" />
        <!--进度条颜色-->
        <attr name="progressColor" format="reference|color" />
        <!--进度条背景颜色-->
        <attr name="progressBackgroundColor" format="reference|color" />
        <!--进度条宽度-->
        <attr name="progressWidth" format="integer" />
        <!-- 动画执行时间-->
        <attr name="animatorDuration" format="integer" />
    </declare-styleable>
</resources>
